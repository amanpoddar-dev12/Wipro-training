@model CarRental.Web.Models.Vehicle
<h2>Add Car</h2>
<form asp-action="Create" method="post">
    <div class="mb-3"><label>Make</label><input asp-for="Make" class="form-control" /></div>
    <div class="mb-3"><label>Model</label><input asp-for="Model" class="form-control" /></div>
    <div class="mb-3"><label>Year</label><input asp-for="Year" class="form-control" /></div>
    <div class="mb-3"><label>Daily Rate</label><input asp-for="DailyRate" class="form-control" /></div>
    <div class="mb-3"><label>Passenger Capacity</label><input asp-for="PassengerCapacity" class="form-control" /></div>
    <div class="mb-3"><label>Engine Capacity</label><input asp-for="EngineCapacity" class="form-control" /></div>
    <button class="btn btn-success">Save</button>
</form>
